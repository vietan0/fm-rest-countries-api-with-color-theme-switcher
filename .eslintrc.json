{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "plugin:react/jsx-runtime"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-console": "off",
    "react/jsx-one-expression-per-line": ["off", { "allow": "single-child" }],
    "object-curly-newline": [
      "error",
      { "minProperties": 4, "ImportDeclaration": { "minProperties": 7 } }
    ]
  }
}
